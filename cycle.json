{
  "version": "1.0",
  "scoped_variables": [
    {
      "identifier": "stackone",
      "scope": {
        "containers": {
          "global": true
        }
      },
      "access": {
        "env_variable": {
          "key": "stackone"
          
        }
      },
      "source": {
        "type": "raw",
        "details": {
          "value": "STACKONEVALUE",
          "secret": null,
          "blob": false
        }
      }
    }
  ],

  "containers": {
    "demo": {
      "name": "demo",
      "image": {
        "name": "simplews",
        "origin": {
          "type": "docker-file",
          "details": {
            "context_dir": "/",
            "build_file": "/Dockerfile"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "simplews",
          "ports": ["80:80"]
        },
        "deploy": {
          "instances": 1
        }
      }
    },
    "reach": {
      "name": "reach",
      "image": {
        "name": "simplews-external",
        "origin": {
          "type": "docker-file",
          "details": {
            "repo": {
              "url": "https://github.com/aubuchcl/build-dir-two.git"
            },
            "context_dir": "/",
            "build_file": "/Dockerfile"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "simplews",
          "ports": ["80:80"]
        },
        "deploy": {
          "instances": 1
        }
      }
    }
  }
}
